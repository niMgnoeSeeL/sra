{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug taintflow.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/rq2/sra/taintflow.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["--some-arg"]
    },
    {
      "name": "Debug formatter",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/formatter/build/myfmt",
      "args": ["${workspaceFolder}/formatter/demo/if.c"],
      "cwd": "${workspaceFolder}/formatter/build",
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "externalConsole": false
    },
  ],
}